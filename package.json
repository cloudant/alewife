{
  "name": "alewife",
  "version": "0.0.1",
  "description": "Code-oriented Documentation Framework",
  "main": "Gruntfile.js",
  "dependencies": {},
  "devDependencies": {
    "async": "~0.2.10",
    "nano": "~4.1.4",
    "couchapp": "0.10.x",
    "dive": "~0.3.0",
    "browserify": "~3.32.1",
    "js-yaml": "~3.0.2"
  },
  "scripts": {
    "yaml": "./node_modules/.bin/js-yaml config.yaml > config.json",
    "test": "./node_modules/.bin/grunt deploy && ./node_modules/.bin/grunt scaffold && ./node_modules/.bin/grunt sync",
    "deploy": "./node_modules/.bin/grunt deploy",
    "sync": "./node_modules/.bin/grunt sync",
    "scaffold": "node tasks/scaffold.js",
    "sitemap": "./node_modules/.bin/grunt show-sitemap",
    "clean": "git clean -dxf",
    "todo": "grep -r TODO docs assets"
  },
